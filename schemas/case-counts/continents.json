{
  "name": "Continents Case Counts",
  "description": "",
  "filename": {
    "regex": "^continents\\.csv$"
  },
  "columns": [
    {
      "type": "enum",
      "name": "continent_code",
      "required": true,
      "null_or_empty": false,
      "values": [
        "AF",
        "AS",
        "EU",
        "NA",
        "OC",
        "SA",
        "AN"
      ]
    },
    {
      "type": "string",
      "name": "date",
      "required": true,
      "null_or_empty": false,
      "regex": "^([0-9]{4})(-)(1[0-2]|0[1-9])\\2(3[01]|0[1-9]|[12][0-9])$",
      "min": 10,
      "max": 10
    },
    {
      "type": "integer",
      "name": "cases",
      "required": true,
      "null_or_empty": false,
      "regex": null,
      "min": 0,
      "max": null
    },
    {
      "type": "integer",
      "name": "deaths",
      "required": true,
      "null_or_empty": false,
      "regex": null,
      "min": 0,
      "max": null
    }
  ]
}
