    {
      "type": "integer",
      "name": "cases",
      "required": true,
      "null_or_empty": true,
      "regex": null,
      "min": 0,
      "max": null
    },
    {
      "type": "integer",
      "name": "deaths",
      "required": true,
      "null_or_empty": true,
      "regex": null,
      "min": 0,
      "max": null
    }
  ]
}
